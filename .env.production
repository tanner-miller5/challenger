# Production environment variables (DO NOT COMMIT WITH REAL VALUES)
COMPOSE_PROJECT_NAME=challenger-prod
POSTGRES_PASSWORD=your-secure-postgres-password-here
JWT_SECRET=your-super-secure-jwt-secret-256-bits-long-here
NODE_ENV=production

# Database
DATABASE_URL=postgresql://challenger_user:your-secure-postgres-password-here@postgres:5432/challenger_db

# API URLs - Update these with your actual domain
REACT_APP_API_URL=http://your-domain.com:5000/api
REACT_APP_UPLOAD_URL=http://your-domain.com:5000

# CORS Origins (for backend)
CORS_ORIGINS=http://your-domain.com,https://your-domain.com
